	GetPlayerFacingAngle(playerid, PlayerInfo[ playerid ][UltimaPosA]);
	PlayerInfo[ playerid ][UInt] = GetPlayerInterior(playerid);
	PlayerInfo[ playerid ][UWorld] = GetPlayerVirtualWorld(playerid);
