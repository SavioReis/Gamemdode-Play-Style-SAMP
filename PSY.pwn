	    getdate(Year, Month, Day);
	}
	new V0,V1,V2,V3,V4;
	new V5[] = {0,31,59,90,120,151,181,212,243,273,304,334};
	V0 = Day + V5[Month - 1];if(Month > 2 && ((Year % 4 == 0 && Year % 100 != 0) || Year % 400 == 0)) V0++;
	V1 = (Year - 1) % 100;V2 = (Year - 1) - V1;V3 = V1 + (V1 / 4);V4 = (((((V2 / 100) % 4) * 5) + V3) % 7);
	V4 += V0 - 1;V4 %= 7;
