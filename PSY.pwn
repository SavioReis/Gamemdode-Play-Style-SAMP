stock StopAudioStreamForAll()
{
    for(new i = 0, e = GetMaxPlayers(); i < e; i++)
	{
        if(IsPlayerConnected(i))
		{
            StopAudioStreamForPlayer(i);
        }
    }
    return 1;
}

stock PlayAudioStreamForAll(inputtext[])
{
    for(new i = 0, e = GetMaxPlayers(); i < e; i++)
	{
        if(IsPlayerConnected(i))
		{
            PlayAudioStreamForPlayer(i, inputtext);
        }
    }
    return 1;
}
