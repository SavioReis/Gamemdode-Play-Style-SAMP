	format(FileBan, 50, PASTA_BANS, GetName(playerid));

	DOF2_CreateFile(FileBan);

	DOF2_SetString(FileBan, "Nome", GetName( playerid ));
	DOF2_SetString(FileBan, "IP", GetIPX( playerid ));
