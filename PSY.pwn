	else SetPlayerPosEx(playerid, InteriorHouse[ (strval(params)) ][h_Px], InteriorHouse[ (strval(params)) ][h_Py], InteriorHouse[ (strval(params)) ][h_Pz], 0.0, InteriorHouse[ (strval(params)) ][h_Int]);
	return true;
}

