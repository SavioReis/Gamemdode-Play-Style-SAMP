        } else {

        GetPlayerPos(playerid, x, y, z);
        GetPlayerFacingAngle(playerid, rot);

        new String[150], File:Csalvos = fopen("logs/Salvos.txt", io_append);
        format(String, sizeof(String), "%s,%f,%f,%f,%f\r\n", coment, x, y, z, rot);
