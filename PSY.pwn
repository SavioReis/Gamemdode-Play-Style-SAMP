    if(strval(params) > (sizeof(InteriorHouse)-1) || 0 > strval(params)) return ErroMSG(playerid, "Interior invalido.");

    if(GetPlayerInterior( playerid ) == 0) GetPlayerPos(playerid, PPos[ playerid ][0], PPos[ playerid ][1], PPos[ playerid ][2]);

