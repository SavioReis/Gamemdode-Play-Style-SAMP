		if(MinerioValue[ playerid ] == 0 || MinerioType[ playerid ] == 0) return ErroMSG(playerid, "Seu minerio nao tem valor, volte la e busque mais!");

        ApplyAnimation(playerid, "CARRY", "putdwn", 3.0, 0, 0, 0, 0, 0);

