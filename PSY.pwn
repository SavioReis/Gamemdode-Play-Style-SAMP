            {
                TimerRespawn[playerid] = SetTimerEx("DestruirCarro", 120000, false, "i", vehicleid);
                return true;
            }
        }
        Loop(vhx, sizeof(VehParamedics))
        {
            if(vehicleid == VehParamedics[vhx])
            {
                TimerRespawn[playerid] = SetTimerEx("DestruirCarro", 120000, false, "i", vehicleid);
                return true;
            }
        }
        Loop(vhx, sizeof(CarrosPM))
