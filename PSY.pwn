                //HouseIcon[proxhouse] = CreateDynamicMapIcon(HouseInfo[proxhouse][hEntranceX], HouseInfo[proxhouse][hEntranceY], HouseInfo[proxhouse][hEntranceZ], 31, -1);

            	return true;
