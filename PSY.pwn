		    HouseInfo[i][hExitY] = DOF2_GetFloat(HFile, "ExitY");
		    HouseInfo[i][hExitZ] = DOF2_GetFloat(HFile, "ExitZ");
		    HouseInfo[i][hSpawnX] = DOF2_GetFloat(HFile, "SpawnX");
