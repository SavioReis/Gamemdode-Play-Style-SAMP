    HouseLabel[proxhouse] = Create3DTextLabel(str, 0xFFFFFF99, HouseInfo[proxhouse][hEntranceX], HouseInfo[proxhouse][hEntranceY], (HouseInfo[proxhouse][hEntranceZ]+0.75), 20.0, 0, 0);
