            EmpInfo[id][EmpLabel] = Create3DTextLabel(str, 0xffffffff, EmpInfo[id][ELocalX], EmpInfo[id][ELocalY], (EmpInfo[id][ELocalZ]+0.15), 25.0, 0, 0);
