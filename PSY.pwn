	if( !PlayerInfo[id][pLogger] ) return ErroMSG( playerid, "Player id inexistente" );
	if(PlayerSolicitou[id] != PizzaJob) return ErroMSG( playerid, "Este player nao solicitou por uma Entrega de Pizza!" );
